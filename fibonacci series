package ques4;

import java.util.Scanner;
public class fibonacci {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	int a=1;
	int b=1;
	int c;
	Scanner input= new Scanner(System.in);
	System.out.print("Enter your ending number for the series ... take your time : ");
	int x=input.nextInt();
	System.out.print(a+" ");
	System.out.print(b+" ");
	while(b<x)
	{
		c=a+b;
		if (c<=x)
		  {
			System.out.print(c+" ");
		  }
		a=b;
		b=c;
	}

	}

}
